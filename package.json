{
  "name": "react-table-ny",
  "description": "React component to display data in a table. Done with TypeScript. Fully responsive and tested. Can be styled.",
  "version": "1.0.2",
  "main": "./dist/umd.js",
  "module": "./dist/es.js",
  "files": [
    "dist",
    "src/components",
    "src/types",
    "src/utils",
    "src/index.ts",
    "src/vite-env.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nabil-Y/react-table-ny.git"
  },
  "author": "nabil-y",
  "keywords": [
    "react-table-ny",
    "react-table",
    "react",
    "table",
    "data-table",
    "react-data",
    "typescript",
    "tsx",
    "ts"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nabil-Y/react-table-ny/issues"
  },
  "homepage": "https://react-table-ny-docs.vercel.app",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^20.0.0",
    "readme-md-generator": "^1.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-dts": "^1.0.4",
    "vite-plugin-libcss": "^1.0.5",
    "vitest": "^0.16.0"
  }
}
